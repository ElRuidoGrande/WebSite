@using OCC.UI.Webhost.Models
<!DOCTYPE html>
<html>
    <head>
        <title>@ViewBag.Title</title>
        <link href="@Url.Content("~/Content/themes/Shared/onetug.113x132.jpg")" rel="shortcut icon"/>
        <link href="@Url.Content("~/Content/css/Layout.css")" rel="stylesheet" type="text/css" />
        <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script> 
        <script src="@Url.Content("~/Scripts/jquery-ui-1.8.11.min.js")" type="text/javascript"></script> 
        
@*        
       <script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.9/jquery-ui.min.js"></script>
*@

        <link href="@Url.Content("~/Content/css/styles.css")" rel="stylesheet" type="text/css" />

        <script type="text/javascript">

            var _gaq = _gaq || [];
            _gaq.push(['_setAccount', 'UA-10607215-2']);
            _gaq.push(['_trackPageview']);

            (function () {
                var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
                ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
                var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
            })();

        </script>
    </head>
    <body>
        <div class="scheduleContainer siteContainer">
            <div id="header">
                <a href= "@Url.Action("Index", "Home")">@Html.ImageTag("../../Content/themes/Metro/Images/CodeCampHeaderLogo.png", "Orlando Code Camp", title: "Orlando Code Camp")</a>
            </div>
            
            <div class="scheduleContainer" >
                @RenderBody()
            </div>
            <div>
            </div>
        </div>
    </body>
</html>
